import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner a=new Scanner(System.in);
		int n=a.nextInt();
		int k=a.nextInt();
		String s="0";
		System.out.println(s);
		while(n>1)
		{
		    int l=s.length();
		    for(int i=0;i<l;i++)
		    if(s.charAt(i)=='0')
		    s=s+"1";
		    else
		    s=s+"0";
		    System.out.println(s);
		    n--;
		}
		System.out.println(s.charAt(k-1));
	}
}
