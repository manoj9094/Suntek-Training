import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    Scanner s = new Scanner (System.in);
    int n = s.nextInt();
    int b = s.nextInt();
    for (int i=b,j=0; j < n; i++,j++)
    {
        b=s.nextInt();
	    if(b!=i+1)
	    {
	    System.out.println (i + 1);
	    break;
	    }
    }
  }
}
